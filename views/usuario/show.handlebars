<div class="row">
	<div class="offset-xl-2 col-xl-8 col-lg-12 col-md-12 col-sm-12 col-12">
		<div class="card">
			<div class="card-body">
				<div class="row mb-4">
					<div class="col-sm-8">
						<h5 class="mb-3">Datos:</h5>                                            
						<h3 class="text-dark mb-1">{{nombre}} {{app}} {{apm}}</h3>
						<div><b>Nickname: </b>{{nickname}}</div>
						<div><b>No. Cédula: </b>{{no_cedula}}</div>
						{{!-- <div>Email: info.com.pl</div>
						<div>Phone: +573-282-9117</div> --}}
					</div>
				</div>
				<div class="table-responsive-sm">
					<table class="table table-striped">
						<thead>
							<tr>
								<th>Nombre</th>
								<th>Descripción</th>
								<th class="right">Última fecha de actualización</th>
							</tr>
						</thead>
						<tbody>
							{{#each formatos}}
							<tr>
								<td class="left strong">{{Plantilla.nombre}}</td>
								<td class="left">{{Plantilla.descripcion}}</td>
								<td class="right">{{updated_at}}</td>
							</tr>
							{{else}}
								<tr>
									<td colspan="3">
										<h3>Sin resultados</h3>
									</td>
								</tr>
							{{/each}}
						</tbody>
					</table>
				</div>
			</div>
			{{!-- <div class="card-footer bg-white">
				<p class="mb-0">2983 Glenview Drive Corpus Christi, TX 78476</p>
			</div> --}}
		</div>
	</div>
</div>