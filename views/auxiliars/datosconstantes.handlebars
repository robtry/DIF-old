<div class="row">
	<!-- ============================================================== -->
	<!-- basic table  -->
	<!-- ============================================================== -->
	<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
		<div class="card">
			<h5 class="card-header">  </h5>
			<div class="card-body">
				<div class="table-responsive">
					<table class="table table-striped table-bordered first">
						<thead>
							<tr>
								<th>Dato</th>
								<th>Multivalor</th>
							</tr>
						</thead>
						<tbody>
							{{#each dato}}
								<tr>
									<td>{{dato}}</td>
									<td>{{multivalor}}</td>
									<td>
										<div class="btn-group ml-auto"> 
                                            <a href="/nna/editar/{{exp}}" class="btn btn-sm btn-outline-light">Editar</a>
											<button class="btn btn-sm btn-outline-light delete-nna" data-id={{exp}}>
												<i class="far fa-trash-alt"></i>
											</button>
                                        </div>
									</td>
								</tr>
							{{else}}
								<p>AÃºn no hay registros</p>
							{{/each}}
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
	<form action="/datosconstantes" method="POST" class="needs-validation" autocomplete="off">
		<br>
		<h3>Agregar Datos</h3>
		<div class="row">
				<div class="col-md-12 mb-3">
					<label for="nombre">Dato</label>
					<input type="text" class="form-control" id="dato" name="dato">
					<p>*Obligatorio</p>
				</div>
				<div class="col-md-4 mb-3">
					<label for="multivalor">Multivalor</label>
					<select class="custom-select d-block w-100" id="multivalor" name="multivalor">
						{{#if sexo}}
							<option value={{sexo}}>{{sexo}}</option>
						{{else}}
							<option value="">Seleccionar</option>
						{{/if}}
						<option value="true">true</option>
						<option value="false">false</option>
					</select>
				</div>
			</div>
			<button class="btn btn-primary btn-lg" type="submit">Agregar</button>

	</form>
</div>
	<!-- ============================================================== -->
	<!-- end basic table  -->
	<!-- ============================================================== -->

